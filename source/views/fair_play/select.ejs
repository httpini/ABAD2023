<!DOCTYPE html>
<html lang="en">
    <%- include("../includes/head.ejs")  %> 

<body>
    <%- include("../includes/header.ejs") %>
    <main>
        <section>
            <h3>Selecciona un torneo para ver su Fair Play</h3>

            <nav>
                <% torneos.forEach(t => { %>
                    <a href="/fair-play/torneo/<%=t.id%>">Fair Play de <%= t.name %> <%= t.temporada %></a>
                    <% }) %>

            </nav>

            
        </section>
        

    </main>
</body>
</html>